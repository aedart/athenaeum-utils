{
    "name": "aedart/athenaeum-utils",
    "description": "Offers a few utility components that are used by the Athenaeum packages",
    "keywords": [
        "Athenaeum",
        "Utilities",
        "Array",
        "Json",
        "Math",
        "Function",
        "Version"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "support": {
        "issues": "https://github.com/aedart/athenaeum/issues",
        "source": "https://github.com/aedart/athenaeum"
    },
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "illuminate/support": "^8.15",
        "jean85/pretty-package-versions": "~1.5",
        "aedart/athenaeum-contracts": "^5.17"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Utils\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "5.17.x-dev"
        }
    }
}
